{
  "name": "hugo-theme-goblindegook",
  "version": "1.0.0",
  "description": "Hugo theme for goblindegook.com.",
  "main": "static/js/index.js",
  "scripts": {
    "build": "concurrently 'npm run build:styles' 'npm run build:ts'",
    "build:styles": "postcss src/css/styles.css --no-map --output static/css/styles.css",
    "build:ts": "rollup -c",
    "watch": "concurrently 'npm run watch:styles' 'npm run watch:ts'",
    "watch:styles": "postcss src/css/styles.css --watch --output static/css/styles.css",
    "watch:ts": "rollup -c rollup.watch.js -w",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Lu√≠s Rodrigues",
  "license": "MIT",
  "devDependencies": {
    "@types/imagesloaded": "^4.1.1",
    "@types/lodash": "^4.14.71",
    "@types/masonry-layout": "^4.0.5",
    "concurrently": "^3.5.0",
    "css-modularscale": "^1.1.1",
    "cssnano": "^3.10.0",
    "normalize.css": "^7.0.0",
    "postcss": "^6.0.8",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^3.0.2",
    "postcss-import": "^10.0.0",
    "rollup": "^0.45.2",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-typescript": "^0.8.1",
    "rollup-plugin-uglify": "^2.0.1",
    "rollup-watch": "^4.3.1",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "tslint": "^5.5.0",
    "tslint-config-standard": "^6.0.1",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "animate.css": "^3.5.2",
    "estimate": "^1.0.0",
    "imagesloaded": "^4.1.3",
    "littlefoot": "^1.0.0-alpha.14",
    "lodash": "^4.17.4",
    "masonry": "^0.0.2",
    "masonry-layout": "^4.2.0",
    "tslib": "^1.7.1",
    "vanilla-lazyload": "^8.0.1"
  }
}
