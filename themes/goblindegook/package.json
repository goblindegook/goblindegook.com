{
  "name": "hugo-theme-goblindegook",
  "version": "1.0.0",
  "description": "Hugo theme for goblindegook.com.",
  "author": "Lu√≠s Rodrigues",
  "license": "MIT",
  "private": true,
  "main": "static/js/index.js",
  "scripts": {
    "prebuild": "yarn test",
    "build": "concurrently 'yarn build:styles' 'yarn build:scripts'",
    "build:styles": "postcss src/css/styles.css --no-map --output static/css/styles.css",
    "build:scripts": "rollup -c",
    "lint": "concurrently 'yarn lint:styles' 'yarn lint:scripts'",
    "lint:styles": "stylelint 'src/**/*.css'",
    "lint:scripts": "eslint '{src,test}/**/*.ts'",
    "watch": "concurrently 'yarn watch:styles' 'yarn watch:scripts' 'yarn test --watch'",
    "watch:styles": "yarn build:styles --watch",
    "watch:scripts": "rollup -c -w",
    "pretest": "yarn lint",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@pacote/eslint-config": "^4.0.0",
    "@pacote/eslint-config-jest": "^1.0.0",
    "@rollup/plugin-alias": "^3.0.1",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/dompurify": "^2.0.1",
    "@types/estimate": "^1.0.0",
    "@types/imagesloaded": "^4.1.1",
    "@types/jest": "^26.0.0",
    "@types/lunr": "^2.1.1",
    "@types/marked": "^0.7.2",
    "@types/masonry-layout": "^4.2.0",
    "@types/node-fetch": "^2.5.4",
    "@types/vfile-message": "^2.0.0",
    "@types/webfontloader": "^1.6.29",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "caniuse-lite": "^1.0.30001088",
    "concurrently": "^5.0.0",
    "css-modularscale": "^1.1.1",
    "cssnano": "^4.1.7",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^15.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-jest-dom": "^3.1.7",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^26.0.1",
    "pixrem": "^5.0.0",
    "postcss": "^8.1.4",
    "postcss-cli": "^8.1.0",
    "postcss-import": "^13.0.0",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^2.0.2",
    "rollup": "^2.2.0",
    "rollup-plugin-uglify": "^6.0.0",
    "stylelint": "^13.1.0",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "ts-jest": "^26.2.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "animate.css": "^4.1.0",
    "barba.js": "^1.0.0",
    "dompurify": "^2.0.3",
    "estimate": "^1.0.0",
    "imagesloaded": "^4.1.4",
    "littlefoot": "^4.0.0-0",
    "lunr": "^2.1.2",
    "marked": "^0.8.0",
    "masonry": "^0.0.2",
    "masonry-layout": "^4.2.1",
    "normalize.css": "^8.0.0",
    "quicklink": "^2.0.0",
    "stylelint-prettier": "^1.1.2",
    "unfetch": "^4.0.1",
    "vanilla-lazyload": "^12.4.0",
    "webfontloader": "^1.6.28"
  }
}
