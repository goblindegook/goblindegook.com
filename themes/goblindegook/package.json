{
  "name": "hugo-theme-goblindegook",
  "version": "1.0.0",
  "description": "Hugo theme for goblindegook.com.",
  "author": "Lu√≠s Rodrigues",
  "license": "MIT",
  "private": true,
  "main": "static/js/index.js",
  "scripts": {
    "prebuild": "npm test",
    "build": "concurrently 'npm run build:styles' 'npm run build:ts'",
    "build:styles": "postcss src/css/styles.css --no-map --output static/css/styles.css",
    "build:ts": "rollup -c",
    "watch": "concurrently 'npm run watch:styles' 'npm run watch:ts' 'npm test -- --watch'",
    "watch:styles": "postcss src/css/styles.css --watch --output static/css/styles.css",
    "watch:ts": "rollup -c rollup.watch.js -w",
    "pretest": "eslint '{src,test}/**/*.ts'",
    "test": "jest"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ]
  },
  "devDependencies": {
    "@types/dompurify": "^0.0.33",
    "@types/imagesloaded": "^4.1.1",
    "@types/jest": "^24.0.11",
    "@types/lunr": "^2.1.1",
    "@types/marked": "^0.6.0",
    "@types/masonry-layout": "^4.2.0",
    "@types/webfontloader": "^1.6.29",
    "@typescript-eslint/eslint-plugin": "^1.11.0",
    "@typescript-eslint/parser": "^1.11.0",
    "caniuse-lite": "^1.0.30000792",
    "concurrently": "^4.1.0",
    "css-modularscale": "^1.1.1",
    "cssnano": "^4.1.7",
    "eslint": "^6.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.0",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "jest": "^24.0.0",
    "pixrem": "^5.0.0",
    "postcss": "^7.0.6",
    "postcss-cli": "^6.0.1",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.6.0",
    "prettier": "^1.18.2",
    "rollup": "^1.1.0",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-node-resolve": "^5.0.0",
    "rollup-plugin-typescript": "^1.0.0",
    "rollup-plugin-uglify": "^6.0.0",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.2.0",
    "ts-jest": "^24.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.2.1"
  },
  "dependencies": {
    "animate.css": "^3.6.1",
    "barba.js": "^1.0.0",
    "dompurify": "^1.0.11",
    "estimate": "^1.0.0",
    "imagesloaded": "^4.1.4",
    "littlefoot": "^2.0.0",
    "lunr": "^2.1.2",
    "marked": "^0.7.0",
    "masonry": "^0.0.2",
    "masonry-layout": "^4.2.1",
    "normalize.css": "^8.0.0",
    "unfetch": "^4.0.1",
    "vanilla-lazyload": "8.2.1",
    "webfontloader": "^1.6.28"
  }
}
